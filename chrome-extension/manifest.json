{
    "manifest_version": 3,
    "name": "Political Bias Rating",
    "version": "15.0",
    "description": "Reads a webpage and generates a political bias rating.",
    "permissions": ["activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  